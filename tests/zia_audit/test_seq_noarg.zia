module Test;

// Test: Seq without size argument

func start() {
    Viper.Terminal.Say("=== Seq No Arg Test ===");

    var s = Viper.Collections.Seq.New();
    Viper.Terminal.Say("Created seq");

    Viper.Collections.Seq.Push(s, "hello");
    Viper.Terminal.Say("Pushed element");

    var len = Viper.Collections.Seq.get_Len(s);
    Viper.Terminal.Say("Len: " + Viper.Strings.FromInt(len));

    if (len == 1) {
        Viper.Terminal.Say("RESULT: ok");
    } else {
        Viper.Terminal.Say("RESULT: fail");
    }
}
