module TestGenericsEntity;

// Generic entity
entity Container[T] {
    expose T value;

    expose func init(v: T) {
        value = v;
    }

    expose func getValue() -> T {
        return value;
    }
}

func start() {
    var intBox: Container[Integer] = new Container[Integer](42);
    if (intBox.getValue() == 42) {
        Viper.Terminal.Say("RESULT: ok");
    } else {
        Viper.Terminal.Say("RESULT: fail");
    }
}
