module TestGenericsValueTypes;

// Test 1: Simple generic value type
value Box[T] {
    expose T contents;

    expose func init(value: T) {
        contents = value;
    }

    expose func get() -> T {
        return contents;
    }
}

func start() {
    var intBox: Box[Integer] = new Box[Integer](42);
    if (intBox.get() == 42) {
        Viper.Terminal.Say("RESULT: ok");
    } else {
        Viper.Terminal.Say("RESULT: fail");
    }
}
