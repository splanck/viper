module Test;

// Test: List using the exact pattern from working code

func main() {
    Viper.Terminal.Say("=== List Pattern Test ===");

    var list = Viper.Collections.List.New();
    Viper.Collections.List.Push(list, Viper.Core.Box.I64(100));

    var item = Viper.Collections.List.Get(list, 0);
    Viper.Terminal.Say("Got item");

    var value = Viper.Core.Box.ToI64(item);
    Viper.Terminal.Say("Value: " + Viper.String.FromInt(value));

    if (value == 100) {
        Viper.Terminal.Say("RESULT: ok");
    } else {
        Viper.Terminal.Say("RESULT: fail");
    }
}
