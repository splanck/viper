module TestListUnbox;

// Test: List with unbox

func start() {
    Viper.Terminal.Say("=== List Unbox Test ===");

    var list: Ptr = Viper.Collections.List.New();
    Viper.Collections.List.Push(list, 100);

    var item: Ptr = Viper.Collections.List.Get(list, 0);
    Viper.Terminal.Say("Got item");

    var value: Integer = Viper.Core.Box.ToI64(item);
    Viper.Terminal.Say("Value: " + Viper.String.FromInt(value));

    if (value == 100) {
        Viper.Terminal.Say("RESULT: ok");
    } else {
        Viper.Terminal.Say("RESULT: fail");
    }
}
