# Viper Web Server Configuration
# Copy this file to server.conf to customize settings

# Server port
port = 8080

# Web root directory (relative to server location or absolute path)
webroot = ./www

# Number of worker threads
workers = 4

# Log level: DEBUG, INFO, WARN, ERROR (or 0-3)
log_level = INFO

# Default index file
index_file = index.html
