module LogDemo;
bind Viper.Terminal;
func start() {
    Say("=== Viper.Log Demo ===");
    SayInt(Viper.Log.Level);
    SayInt(Viper.Log.DEBUG);
    SayInt(Viper.Log.INFO);
    SayBool(Viper.Log.Enabled(Viper.Log.INFO));
    Viper.Log.Info("test info message");
    Viper.Log.Debug("test debug message");
    Say("done");
}
