module TweenDemo;
bind Viper.Terminal;
func start() {
    Say("=== Viper.Game.Tween Demo ===");
    var tw = new Viper.Game.Tween();
    SayBool(tw.IsRunning);
    SayBool(tw.IsComplete);
    tw.Start(0.0, 100.0, 60, 0);
    SayBool(tw.IsRunning);
    SayInt(tw.Duration);
    SayBool(tw.Update());
    SayInt(tw.Elapsed);
    tw.Stop();
    tw.Reset();
    Say("done");
}
