module QuadtreeDemo;
bind Viper.Terminal;
func start() {
    Say("=== Viper.Game.Quadtree Demo ===");
    var qt = new Viper.Game.Quadtree(0, 0, 100, 100);
    SayInt(qt.ItemCount);
    SayBool(qt.Insert(1, 10, 10, 5, 5));
    SayBool(qt.Insert(2, 50, 50, 5, 5));
    SayInt(qt.ItemCount);
    SayInt(qt.QueryRect(0, 0, 100, 100));
    SayInt(qt.GetResult(0));
    SayBool(qt.Remove(1));
    SayInt(qt.ItemCount);
    qt.Clear();
    SayInt(qt.ItemCount);
    Say("done");
}
