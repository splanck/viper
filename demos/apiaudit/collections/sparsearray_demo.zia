module SparseArrayDemo;
bind Viper.Terminal;
func start() {
    Say("=== Viper.Collections.SparseArray Demo ===");
    var s = new Viper.Collections.SparseArray();
    s.Set(0, "zero"); s.Set(100, "hundred"); s.Set(999, "big");
    SayInt(s.Len);
    SayBool(s.Has(100));
    SayBool(s.Has(50));
    s.Remove(100);
    SayInt(s.Len);
    s.Clear();
    SayInt(s.Len);
    Say("done");
}
