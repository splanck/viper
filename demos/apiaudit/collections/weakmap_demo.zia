module WeakMapDemo;
bind Viper.Terminal;
func start() {
    Say("=== Viper.Collections.WeakMap Demo ===");
    var w = new Viper.Collections.WeakMap();
    w.Set("a", "alpha"); w.Set("b", "beta");
    SayInt(w.Len);
    SayBool(w.Has("a"));
    SayBool(w.Has("z"));
    w.Remove("a");
    SayInt(w.Len);
    w.Clear();
    SayBool(w.IsEmpty);
    Say("done");
}
