module DefaultMapDemo;
bind Viper.Terminal;
func start() {
    Say("=== Viper.Collections.DefaultMap Demo ===");
    var d = new Viper.Collections.DefaultMap("N/A");
    d.Set("name", "viper");
    SayInt(d.Len);
    SayBool(d.Has("name"));
    SayBool(d.Has("age"));
    d.Remove("name");
    SayInt(d.Len);
    d.Clear();
    SayInt(d.Len);
    Say("done");
}
