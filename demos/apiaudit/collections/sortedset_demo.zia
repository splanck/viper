module SortedSetDemo;
bind Viper.Terminal;
func start() {
    Say("=== Viper.Collections.SortedSet Demo ===");
    var s = new Viper.Collections.SortedSet();
    s.Put("c"); s.Put("a"); s.Put("b"); s.Put("a");
    SayInt(s.Len);
    SayBool(s.Has("a"));
    s.Drop("b");
    SayInt(s.Len);
    Say(s.First());
    Say(s.Last());
    s.Clear();
    SayBool(s.IsEmpty);
    Say("done");
}
