module DequeDemo;
bind Viper.Terminal;
func start() {
    Say("=== Viper.Collections.Deque Demo ===");
    var d = new Viper.Collections.Deque();
    d.PushBack("a"); d.PushBack("b"); d.PushFront("z");
    SayInt(d.Len);
    SayBool(d.IsEmpty);
    d.Clear();
    SayBool(d.IsEmpty);
    Say("done");
}
