module MultiMapDemo;
bind Viper.Terminal;
func start() {
    Say("=== Viper.Collections.MultiMap Demo ===");
    var m = new Viper.Collections.MultiMap();
    m.Put("fruit", "apple"); m.Put("fruit", "banana"); m.Put("color", "red");
    SayInt(m.Len);
    SayInt(m.KeyCount);
    SayInt(m.CountFor("fruit"));
    SayBool(m.Has("fruit"));
    m.RemoveAll("fruit");
    SayInt(m.Len);
    m.Clear();
    SayBool(m.IsEmpty);
    Say("done");
}
