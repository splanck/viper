module FrozenMapDemo;
bind Viper.Terminal;
func start() {
    Say("=== Viper.Collections.FrozenMap Demo ===");
    var keys = new Viper.Collections.Seq();
    keys.Push("a"); keys.Push("b"); keys.Push("c");
    var vals = new Viper.Collections.Seq();
    vals.Push("alpha"); vals.Push("beta"); vals.Push("gamma");
    var fm = new Viper.Collections.FrozenMap(keys, vals);
    SayInt(fm.Len);
    SayBool(fm.Has("a"));
    SayBool(fm.Has("z"));
    SayBool(fm.IsEmpty);
    Say("done");
}
