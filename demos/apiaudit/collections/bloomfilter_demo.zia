module BloomFilterDemo;
bind Viper.Terminal;
func start() {
    Say("=== Viper.Collections.BloomFilter Demo ===");
    var bf = new Viper.Collections.BloomFilter(1000, 0.01);
    bf.Add("apple"); bf.Add("banana"); bf.Add("cherry");
    SayInt(bf.Count);
    SayBool(bf.MightContain("apple"));
    SayBool(bf.MightContain("grape"));
    bf.Clear();
    SayInt(bf.Count);
    Say("done");
}
