module CountMapDemo;
bind Viper.Terminal;
func start() {
    Say("=== Viper.Collections.CountMap Demo ===");
    var c = new Viper.Collections.CountMap();
    c.Inc("apple"); c.Inc("apple"); c.Inc("banana");
    SayInt(c.Len);
    SayInt(c.Total);
    SayInt(c.Get("apple"));
    SayBool(c.Has("banana"));
    c.Dec("apple");
    SayInt(c.Get("apple"));
    c.Remove("banana");
    SayInt(c.Len);
    c.Clear();
    SayBool(c.IsEmpty);
    Say("done");
}
