module BiMapDemo;
bind Viper.Terminal;
func start() {
    Say("=== Viper.Collections.BiMap Demo ===");
    var b = new Viper.Collections.BiMap();
    b.Put("en", "English"); b.Put("fr", "French"); b.Put("de", "German");
    SayInt(b.Len);
    Say(b.GetByKey("en"));
    Say(b.GetByValue("French"));
    SayBool(b.HasKey("fr"));
    SayBool(b.HasValue("Spanish"));
    b.RemoveByKey("de");
    SayInt(b.Len);
    b.Clear();
    SayBool(b.IsEmpty);
    Say("done");
}
