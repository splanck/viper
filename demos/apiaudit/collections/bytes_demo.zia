module BytesDemo;
bind Viper.Terminal;
func start() {
    Say("=== Viper.Collections.Bytes Demo ===");
    var b = new Viper.Collections.Bytes(8);
    SayInt(b.Len);
    b.Set(0, 72); b.Set(1, 101); b.Set(2, 108); b.Set(3, 108); b.Set(4, 111);
    SayInt(b.Get(0));
    SayInt(b.Find(108));
    Say(b.ToHex());
    b.Fill(0);
    SayInt(b.Get(0));
    Say("done");
}
