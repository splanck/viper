module SetDemo;
bind Viper.Terminal;
func start() {
    Say("=== Viper.Collections.Set Demo ===");
    var s = new Viper.Collections.Set();
    s.Add("a"); s.Add("b"); s.Add("c"); s.Add("a");
    SayInt(s.Len);
    SayBool(s.Has("a"));
    SayBool(s.Has("z"));
    s.Remove("b");
    SayInt(s.Len);
    s.Clear();
    SayBool(s.IsEmpty);
    Say("done");
}
