module LruCacheDemo;
bind Viper.Terminal;
func start() {
    Say("=== Viper.Collections.LruCache Demo ===");
    var c = new Viper.Collections.LruCache(3);
    c.Put("a", "alpha"); c.Put("b", "beta"); c.Put("c", "gamma");
    SayInt(c.Len);
    SayInt(c.Cap);
    SayBool(c.Has("a"));
    c.Put("d", "delta");
    SayInt(c.Len);
    SayBool(c.Has("a"));
    c.Remove("b");
    SayInt(c.Len);
    c.Clear();
    SayBool(c.IsEmpty);
    Say("done");
}
