module SeqDemo;
bind Viper.Terminal;

func start() {
    Say("=== Viper.Collections.Seq Demo ===");
    var s = new Viper.Collections.Seq();
    s.Push("a");
    s.Push("b");
    s.Push("c");
    SayInt(s.Len);
    SayInt(s.Cap);
    SayBool(s.IsEmpty);
    SayBool(s.Has("b"));
    SayInt(s.Find("c"));
    s.Reverse();
    s.Clear();
    SayBool(s.IsEmpty);
    Say("done");
}
