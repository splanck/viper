module UnionFindDemo;
bind Viper.Terminal;
func start() {
    Say("=== Viper.Collections.UnionFind Demo ===");
    var uf = new Viper.Collections.UnionFind(10);
    uf.Union(0, 1);
    uf.Union(2, 3);
    uf.Union(1, 3);
    SayBool(uf.Connected(0, 3));
    SayBool(uf.Connected(0, 5));
    SayInt(uf.Count);
    SayInt(uf.Find(3));
    SayInt(uf.SetSize(0));
    Say("done");
}
