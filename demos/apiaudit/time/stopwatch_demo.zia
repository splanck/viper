module StopwatchDemo;
bind Viper.Terminal;
func start() {
    Say("=== Viper.Time.Stopwatch Demo ===");
    var sw = new Viper.Time.Stopwatch();
    SayBool(sw.IsRunning);
    sw.Start();
    SayBool(sw.IsRunning);
    sw.Stop();
    SayBool(sw.IsRunning);
    sw.Reset();
    SayInt(sw.ElapsedMs);
    Say("done");
}
