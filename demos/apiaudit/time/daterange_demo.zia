module DateRangeDemo;
bind Viper.Terminal;
func start() {
    Say("=== Viper.Time.DateRange Demo ===");
    var start = Viper.Time.DateTime.Create(2025, 1, 1, 0, 0, 0);
    var end_ = Viper.Time.DateTime.Create(2025, 1, 31, 23, 59, 59);
    var r = new Viper.Time.DateRange(start, end_);
    SayInt(r.Start);
    SayInt(r.Days());
    var mid = Viper.Time.DateTime.Create(2025, 1, 15, 12, 0, 0);
    SayBool(r.Contains(mid));
    Say(r.ToString());
    Say("done");
}
