module ObjectDemo;
bind Viper.Terminal;

func start() {
    Say("=== Viper.Core.Object Demo ===");
    var list = new Viper.Collections.List();
    Say(Viper.Core.Object.ToString(list));
    Say(Viper.Core.Object.TypeName(list));
    SayInt(Viper.Core.Object.TypeId(list));
    SayBool(Viper.Core.Object.IsNull(list));
    Say("done");
}
