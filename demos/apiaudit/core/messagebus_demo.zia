module MessageBusDemo;
bind Viper.Terminal;
func start() {
    Say("=== Viper.Core.MessageBus Demo ===");
    var bus = new Viper.Core.MessageBus();
    SayInt(bus.TotalSubscriptions);
    SayInt(bus.SubscriberCount("test"));
    bus.ClearTopic("test");
    bus.Clear();
    SayInt(bus.TotalSubscriptions);
    Say("done");
}
