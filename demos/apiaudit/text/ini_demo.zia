module IniDemo;
bind Viper.Terminal;
func start() {
    Say("=== Viper.Text.Ini Demo ===");
    var doc = Viper.Text.Ini.Parse("[section]\nkey = value\n");
    Say(Viper.Text.Ini.Get(doc, "section", "key"));
    SayBool(Viper.Text.Ini.HasSection(doc, "section"));
    SayBool(Viper.Text.Ini.HasSection(doc, "other"));
    Say("done");
}
