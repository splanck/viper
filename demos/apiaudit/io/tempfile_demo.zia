module TempFileDemo;
bind Viper.Terminal;
func start() {
    Say("=== Viper.IO.TempFile Demo ===");
    Say(Viper.IO.TempFile.Dir());
    var p = Viper.IO.TempFile.Path();
    SayBool(Viper.String.Len(p) > 0);
    var pp = Viper.IO.TempFile.PathWithPrefix("test_");
    SayBool(Viper.String.Has(pp, "test_"));
    Say("done");
}
