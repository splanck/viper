#ifndef _LIBCPP_CTIME
#define _LIBCPP_CTIME

#include "../time.h"

namespace std
{

/* Types */
using ::clock_t;
using ::time_t;
using ::timespec;
using ::tm;

/* Time manipulation */
using ::clock;
using ::difftime;
using ::mktime;
using ::time;

/* Conversions */
using ::asctime;
using ::ctime;
using ::gmtime;
using ::localtime;
using ::strftime;

/* POSIX extensions */
using ::clock_getres;
using ::clock_gettime;
using ::clock_settime;
using ::nanosleep;
using ::timespec_get;

} /* namespace std */

#endif /* _LIBCPP_CTIME */
